@page
@model ResumеBuilder.Pages.ResumeTemp2
@{
}
<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Resume</title>
        <script src="https://unpkg.com/feather-icons"></script>
        <link rel="stylesheet" href="Style2.css">
    </head>

    
    <body>
        <form method="post">
            <div class="container">

                <div class="group-1">
                    <div>
                        <h1 class="profile_name">
                             @HttpContext.Session.GetString("_FullName")
                        </h1>
                    </div>
                    <hr>

                    <div class="contact">
                        <h3 class="title">Contact</h3>
                        <div class="contact_info">
                            <p class="description">
                                @HttpContext.Session.GetString("_Address")
                            </p>
                            <p class="description">
                                @HttpContext.Session.GetString("_PhoneNumber")
                            </p>
                            <p class="description">
                                @HttpContext.Session.GetString("_Email")
                            </p>
                        </div>
                    </div>
                    <script>
                        feather.replace()
                    </script>

                    <div class="interetss">
                        <h3 class="title">Interests</h3>
                        <textarea class="textarea1" placeholder="Interests..." rows="1" cols="30"></textarea>
                    </div>
                </div>




                <div class="group-2">
                    <div class="title">
                        <h3>Skills</h3>
                        <textarea class="textarea2" placeholder="Skills..." rows="1" cols="30"></textarea>
                    </div>

                    <div class="edu">
                        <h3 class="title">Education</h3>
                        <textarea class="textarea2" placeholder="Education..." rows="1" cols="30"></textarea>
                    </div>

                    <div class="exp">
                        <h3 class="title">Experience</h3>
                        <textarea class="textarea2" placeholder="Experience..." rows="1" cols="30"></textarea>
                    </div>
                </div>

            </div>
        </form>
    </body>
</html>
